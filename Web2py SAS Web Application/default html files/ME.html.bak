{{extend 'layout.html'}}
<h3>Manage Experiments:</h3>
{{if rows:}}
    <div style="overflow: auto;" width="80%">
    <table>
    <tr> <th> Experiment Name </th>
    {{
        for row in rows:
        =TR(TD(row.expName), A(str(T('Edit')),_href=URL('editExperiment',args=row.id)), A(str(T('Delete')),_href=URL('deleteExperiment',args=row.id)), A(str(T('run')),_href=URL('jumpToExp',args=row.id)))
        pass 
    }}
    </table>


    </div>
{{pass}}
<h4>
[{{=A(str(T('Create new Experiment')),_href=URL('createExperiment'))}} ]<br/><br/>
</h4>
<h4>
<form>
    <input type="button"value="Run the recently used Experiment"
    onclick="document.location='{{=URL(r=request,f='jumpToExp')}}'">
</form>
</h4>
